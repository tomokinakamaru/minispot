{
  "ServerApp": {
    "ip": "*",
    "allow_root": true,
    "answer_yes": true,
    "autoreload": true,
    "open_browser": false,
    "kernel_manager_class": "minispot.kernelmanager.KernelManager",
    "jpserver_extensions": {
      "jupyterlab": false,
      "jupyter_server_terminals": false,
      "jupyter_lsp": true,
      "notebook": true,
      "notebook_shim": false
    }
  },
  "KernelSpecManager": {
    "allowed_kernelspecs": ["python-isolated"],
    "ensure_native_kernel": false
  }
}
